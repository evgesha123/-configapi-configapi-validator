{
  "name": "@configapi/configapi-validator",
  "version": "0.0.0",
  "description": "Various packages to build OpenAPI APIs in node",
  "main": "index.js",
  "private": false,
  "publishConfig":{
    "@configapi:registry": "https://gitlab.com/api/v4/projects/170/packages/npm/"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "clean": "lerna clean",
    "cover": "lerna run cover",
    "combine-coverage": "istanbul-combine packages/*/coverage/coverage*.json",
    "coveralls": "cat ./coverage/lcov.info | coveralls -v",
    "lint": "tslint -c tslint.json './packages/**/*.[j,t]s'",
    "test": "lerna run test",
    "publish": "lerna publish"
  },
  "keywords": [
    "openapi",
    "tools"
  ],
  "author": "Joseph Spencer, copied by Eugene Leonov",
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "^7.0.0",
    "@types/node": "14.0.4",
    "bluebird": "3.7.2",
    "body-parser": "^1.14.2",
    "chai": "4.2.0",
    "cors": "^2.7.1",
    "coveralls": "3.0.12",
    "express": "^4.13.3",
    "glob": "7.1.6",
    "istanbul": "0.4.5",
    "istanbul-combine": "^0.0.1",
    "lerna": "^5.0.0",
    "mocha": "7.1.2",
    "nyc": "15.1.0",
    "prettier": "^2.0.0",
    "source-map-support": "0.5.17",
    "supertest": "4.0.1",
    "ts-node": "8.8.2",
    "tslint": "6.1.2",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.3.0",
    "typescript": "^4.2.4"
  }
}
